{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "portal/components/*":["components/*"],
            "portal/pages/*":["pages/*"],
            "portal/redux/*":["redux/*"],
            "portal/styles/*":["styles/*"],
            "portal/theme/*":["theme/*"],
            "portal/utils/*":["utils/*"],
            "portal/types/*":["components/type/*"],
            "portal/*":["*"],
            "portal/components":["components/index"],
            "portal/utils":["utils/index"],
            "portal/types":["components/type/index"],
        }
    },
    "exclude": ["node_modules"],
    "include": ["**/*.js", "**/*.jsx"]
}